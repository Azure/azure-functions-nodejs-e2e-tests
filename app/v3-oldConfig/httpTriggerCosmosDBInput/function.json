{
    "bindings": [
        {
            "authLevel": "anonymous",
            "type": "httpTrigger",
            "direction": "in",
            "name": "req",
            "methods": ["get", "post"]
        },
        {
            "type": "http",
            "direction": "out",
            "name": "res"
        },
        {
            "type": "cosmosDB",
            "name": "inputDoc",
            "direction": "in",
            "connectionStringSetting": "CosmosDBConnection",
            "databaseName": "e2eTestCosmosDB",
            "collectionName": "e2eTestContainerTriggerAndOutput",
            "partitionKey": "testPartKey",
            "id": "{Query.id}"
        }
    ],
    "scriptFile": "../dist/httpTriggerCosmosDBInput/index.js"
}
