{
    "bindings": [
        {
            "authLevel": "anonymous",
            "type": "httpTrigger",
            "direction": "in",
            "name": "req",
            "methods": ["get", "post"]
        },
        {
            "type": "http",
            "direction": "out",
            "name": "res"
        },
        {
            "type": "cosmosDB",
            "name": "outputDoc",
            "direction": "out",
            "connectionStringSetting": "CosmosDBConnection",
            "databaseName": "e2eTestDB",
            "collectionName": "e2eTestContainerTriggerAndOutput"
        }
    ],
    "scriptFile": "../dist/httpTriggerCosmosDBOutput/index.js"
}
