    },
    "bugs": {
        "url": "https://github.com/Azure/azure-functions-nodejs-e2e-tests/issues"
    },
    "main": "./dist/azure-functions.js",
    "types": "types/index.d.ts",
    "scripts": {
        "build": "tsc",
        "clean": "rimraf out",
        "lint": "eslint . --fix",
        "watch": "tsc -w",
        "createResources": "node out/resources/createResources.js",
        "deleteResources": "node out/resources/deleteResources.js",
        "test": "npm run testV3 && npm run testV4",
        "testV3": "node out/index.js -m v3",
        "testV4": "node out/index.js -m v4"
    },
